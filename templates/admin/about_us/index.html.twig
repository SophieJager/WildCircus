{% extends 'admin/base.html.twig' %}

{% block title %}AboutUs index{% endblock %}

{% block body %}
    <h1>AboutUs index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Description</th>
                <th>Active</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for about_u in aboutuses %}
            <tr>
                <td>{{ about_u.id }}</td>
                <td>{{ about_u.title }}</td>
                <td>{{ about_u.description }}</td>
                <td>{{ about_u.active }}</td>
                <td>
                    <a href="{{ path('about_us_show', {'id': about_u.id}) }}">show</a>
                    <a href="{{ path('about_us_edit', {'id': about_u.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('about_us_new') }}">Create new</a>
{% endblock %}
